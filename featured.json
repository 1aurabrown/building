---
layout: null
---
[
{% for post in site.posts %}
    {% if post.tags contains "featured" %}
        {
          "title": "{{ post.title }}",
          "url": "{{ post.url }}",
          "category": "{{ post.categories }}"
          "image": "{{ post.image }}"
          "date": "{{ post.date | date: "%B %d, %Y" }}",
        } {% if forloop.last %}{% else %},{% endif %}
    {% endif %}
{% endfor %}
]